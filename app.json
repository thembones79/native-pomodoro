{
  "expo": {
    "name": "Pomodoro Clock",
    "slug": "native-pomodoro",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "android": {
      "package": "com.michalszumnarski.pomodoroclock",
      "versionCode": 4
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.2.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "My version of Pomodoro Clock in React Native",
    "githubUrl": "https://github.com/thembones79/native-pomodoro"
  }
}